        <Param name="Each Frame" updates="constant" val="import numpy as np &amp;#10;from psychopy import visual, core, event&amp;#10;&amp;#10;### Hide mouse at start of trial&amp;#10;if probe_word_2.status == NOT_STARTED:&amp;#10;    event.Mouse(visible = False)&amp;#10;&amp;#10;### Show mouse and set mouse position, reset tclock component for timing&amp;#10;if probe_word_2.status == STARTED and check_time == 0:&amp;#10;    event.Mouse(visible = True)&amp;#10;    mouse.setPos([0, -.95])&amp;#10;    tclock.reset()&amp;#10;    check_time = check_time + 1&amp;#10;&amp;#10;## If mouse hasn't moved after specified time, add 1 to mouse_test&amp;#10;## (which leads to the execution of the 'warning' routine&amp;#10;if tclock.getTime() &gt; .6 and mouse_tester_2.contains(mouse):&amp;#10;    mouse_test = mouse_test + 1&amp;#10;&amp;#10;&amp;#10;### Sets parameters for true and false criteria by condition&amp;#10;if condition == &quot;tc&quot;:&amp;#10;    if mouse.isPressedIn(true_rect_2, buttons=[0]):&amp;#10;        continueRoutine=False&amp;#10;    if mouse.isPressedIn(false_rect_2, buttons=[0]):&amp;#10;        continueRoutine=True&amp;#10;if condition == &quot;ti&quot;:&amp;#10;    if mouse.isPressedIn(true_rect_2, buttons=[0]):&amp;#10;        continueRoutine=False&amp;#10;    if mouse.isPressedIn(false_rect_2, buttons=[0]):&amp;#10;        continueRoutine=True&amp;#10;if condition == &quot;te&quot;:&amp;#10;    if mouse.isPressedIn(true_rect_2, buttons=[0]):&amp;#10;        continueRoutine=False&amp;#10;    if mouse.isPressedIn(false_rect_2, buttons=[0]):&amp;#10;        continueRoutine=True&amp;#10;if condition == &quot;fc&quot;:&amp;#10;    if mouse.isPressedIn(false_rect_2, buttons=[0]):&amp;#10;        continueRoutine=False&amp;#10;    if mouse.isPressedIn(true_rect_2, buttons=[0]):&amp;#10;        continueRoutine=True&amp;#10;if condition == &quot;fi&quot;:&amp;#10;    if mouse.isPressedIn(false_rect_2, buttons=[0]):&amp;#10;        continueRoutine=False&amp;#10;    if mouse.isPressedIn(true_rect_2, buttons=[0]):&amp;#10;        continueRoutine=True&amp;#10;if condition == &quot;fe&quot;:&amp;#10;    if mouse.isPressedIn(false_rect_2, buttons=[0]):&amp;#10;        continueRoutine=False&amp;#10;    if mouse.isPressedIn(true_rect_2, buttons=[0]):&amp;#10;        continueRoutine=True&amp;#10;&amp;#10;## Add data to data file &amp;#10;if (condition == &quot;tc&quot; or condition == &quot;ti&quot; or condition == &quot;te&quot;) and mouse.isPressedIn(true_rect_2, buttons=[0]):&amp;#10;    accuracy = &quot;correct&quot;&amp;#10;if (condition == &quot;fc&quot; or condition == &quot;fi&quot; or condition == &quot;fe&quot;) and mouse.isPressedIn(false_rect_2, buttons=[0]):&amp;#10;    accuracy = &quot;correct&quot;&amp;#10;if (condition == &quot;tc&quot; or condition == &quot;ti&quot; or condition == &quot;te&quot;) and mouse.isPressedIn(false_rect_2, buttons=[0]):&amp;#10;    accuracy = &quot;incorrect&quot;&amp;#10;if (condition == &quot;fc&quot; or condition == &quot;fi&quot; or condition == &quot;fe&quot;) and mouse.isPressedIn(true_rect_2, buttons=[0]):&amp;#10;    accuracy = &quot;incorrect&quot;&amp;#10;&amp;#10;&amp;#10;## Ends routine after presentation of 'wrong' feedback&amp;#10;if wrong_2.status == FINISHED:&amp;#10;    continueRoutine=False" valType="extendedCode"/>
